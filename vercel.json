    {
        "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
        "rewrites": [
            {
                "source": "/api/(.*)",
                "destination": "https://your-api.domain/$1"
            },
            {
                "source": "/(.*)",
                "destination": "/"
            }
        ]
    }
    